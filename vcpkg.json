{
  "name": "maze",
  "version-string": "latest",
  "dependencies": [
    {
      "name": "glad",
      "features": [
        "gl-api-latest",
        "loader"
      ],
      "version>=": "0.1.36"
    },
    {
      "name": "glm",
      "version>=": "1.0.1"
    },
    {
      "name": "imgui",
      "features": [
        "opengl3-binding",
        "sdl2-binding"
      ],
      "version>=": "1.91.0"
    },
    {
      "name": "meshoptimizer",
      "version>=": "0.20"
    },
    {
      "name": "sdl2",
      "version>=": "2.30.6",
      "features": [
        {
          "name": "x11",
          "platform": "linux"
        },
        {
          "name": "wayland",
          "platform": "linux"
        },
        {
          "name": "ibus",
          "platform": "linux"
        }
      ],
      "platform": "!wasm32"
    },
    {
      "name": "spdlog",
      "version>=": "1.14.1"
    },
    {
      "name": "stb",
      "version>=": "2023-04-11"
    },
    {
      "name": "tinyobjloader",
      "version>=": "2.0.0rc13"
    }
  ]
}
